#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules/@colyseus/loadtest/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules/@colyseus/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules/@colyseus/loadtest/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules/@colyseus/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+loadtest@0.15.7/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@colyseus/loadtest/bin.js" "$@"
else
  exec node  "$basedir/../@colyseus/loadtest/bin.js" "$@"
fi
