#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules/@colyseus/tools/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules/@colyseus/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules/@colyseus/tools/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules/@colyseus/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/@colyseus+tools@0.15.49_@colyseus+core@0.15.55_@colyseus+ws-transport@0.15.3_@colyseus+core@0_gsqlmg3gtxxjexk52ltvf7ci3e/node_modules:/Users/user/Documents/Projects/official/colyseus-test/colyseus-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@colyseus/tools/post-deploy.js" "$@"
else
  exec node  "$basedir/../@colyseus/tools/post-deploy.js" "$@"
fi
